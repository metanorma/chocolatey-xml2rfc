<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>xml2rfc</id>
    <version>3.29.0</version>
    <authors>IETF</authors>
    <owners>IETF</owners>
    <title>XML2RFC - IETF RFC Document Generator</title>
    <description>xml2rfc is a tool used to process XML-based RFC documents into formats such as plain text, HTML, or PDF.</description>
    <summary>Convert XML RFC documents to various output formats.</summary>
    <projectUrl>https://xml2rfc.tools.ietf.org/</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/ietf-tools/common/main/assets/logos/xml2rfc.svg</iconUrl>
    <licenseUrl>https://github.com/ietf-tools/xml2rfc/blob/main/LICENSE</licenseUrl>
    <packageSourceUrl>https://github.com/metanorma/chocolatey-xml2rfc</packageSourceUrl>
    <projectSourceUrl>https://github.com/ietf-tools/xml2rfc</projectSourceUrl>
    <docsUrl>https://authors.ietf.org/en/rfcxml-vocabulary</docsUrl>
    <mailingListUrl>https://github.com/ietf-tools/xml2rfc/discussions</mailingListUrl>
    <bugTrackerUrl>https://github.com/ietf-tools/xml2rfc/issues</bugTrackerUrl>
    <releaseNotes>https://github.com/ietf-tools/xml2rfc/blob/main/CHANGELOG.md</releaseNotes>
    <tags>ietf xml rfc xml2rfc</tags>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="python3" version="[3.0.1,)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools"/>
  </files>
</package>
